{
    "name" : "makinacorpus/goat-mapper",
    "type": "library",
    "description": "Goat mapper is not an ORM, but it provides readonly ORM-like features",
    "keywords": ["orm", "postgresql", "mysql", "database", "symfony", "pdo", "orm"],
    "homepage": "http://github.com/makinacorpus/goat-mapper",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name" : "Pierre RINEAU",
            "email" : "pierre.rineau@processus.org"
        },
        {
            "name" : "Makina Corpus",
            "homepage" : "https://www.makina-corpus.com"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload" : {
        "psr-4" : {
            "Goat\\Mapper\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goat\\Mapper\\Tests\\": "tests/"
        }
    },
    "require" : {
        "php" : ">=7.4",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.8",
        "ocramius/generated-hydrator": "^3.1",
        "makinacorpus/goat-query": "^1.0@alpha",
        "ocramius/proxy-manager": "^2.7"
    },
    "require-dev" : {
        "phpunit/phpunit" : "7.*",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/dependency-injection": "^4.2|^5.0",
        "symfony/event-dispatcher": "^4.0|^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0"
    }
}
